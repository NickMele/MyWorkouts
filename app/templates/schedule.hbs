<ul class="weekdays-list">
	{{#each weekday in weekdays}}
		<li>
			<div class="weekday">{{weekday.day}}Monday</div>
			<div class="date">Jan 29</div>
			<div class="routine">{{weekday.routine}}</div>
			<div class="workouts">{{weekdays.workouts}} Workouts</div>
			<div class="status-container skipped">
				<span>&cross;</span>
			</div>
		</li>
	{{/each}}
</ul>